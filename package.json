{
  "name": "nestjs-skeleton",
  "version": "1.0.1",
  "description": "Nestjs Skeleton",
  "author": "Mamadou Abass DIALLO",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typeorm"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "set NODE_ENV=dev&& nest start src/main.ts --watch",
    "start:dev:nodemon": "set NODE_ENV=dev&& nodemon src/main.ts",
    "start:prod": "set NODE_ENV=prod&& nodemon src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "test": "NODE_ENV=testing jest -w 1 --config ./jest.config.json --forceExit",
    "test:watch": "NODE_ENV=testing jest -w 1 --watch --forceExit",
    "webpack": "webpack --config webpack.config.js",
    "start:hmr": "node dist/main.js",
    "now-build": "yarn --only=dev --no-shrinkwrap && yarn build && yarn gen",
    "dump": "ts-node src/dump"
  },
  "dependencies": {
    "@nestjs/bull": "^0.0.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/typeorm": "^7.1.0",
    "assert": "^2.0.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.13.0",
    "cache-manager": "^2.10.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cron": "^1.7.2",
    "csurf": "^1.10.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.0.0",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "handlebars": "^4.4.5",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "jest-config": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.3",
    "mongoose": "^6.5.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemailer-express-handlebars": "^3.1.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-google-token": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4",
    "uuid": "^3.3.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/testing": "^6.8.3",
    "@types/bull": "^3.12.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/mongodb": "^3.3.6",
    "@types/node": "14.18.10",
    "@types/nodemailer": "^6.2.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "codecov": "^3.6.1",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.7",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-loader-helper": "^1.0.0",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.19.1"
  },
  "lint-staged": {
    "*.ts": [
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
