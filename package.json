{
  "name": "albert-royer",
  "version": "1.0.1",
  "description": "Albert Royer Project",
  "author": "Mamadou Abass DIALLO",
  "keywords": [
    "nestjs"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build:dev": "SET NODE_ENV=dev&&nest build",
    "build:prod": "export NODE_ENV=prod&&nest build",
    "build:stag": "export NODE_ENV=stag&&nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local:dev-webpack": "export NODE_ENV=dev&&nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:local:dev": "export NODE_ENV=dev&&ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:local:stag": "export NODE_ENV=stag&&ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:local:prod": "export NODE_ENV=prod&&nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod:linux": "export NODE_ENV=prod&&node dist/main",
    "start:stag:linux": "export NODE_ENV=stag&&ts-node -r tsconfig-paths/register src/main.ts",
    "gen": "ts-node src/generator/generate-typings.ts",
    "start:prod:win": "SET NODE_ENV=prod&&node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.0.3",
    "@nestjs/cli": "8.2.8",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/typeorm": "^7.1.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "assert": "^2.0.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.13.0",
    "cache-manager": "^2.10.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cron": "^1.7.2",
    "csurf": "^1.10.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.0.0",
    "fs": "^0.0.1-security",
    "generate-password": "^1.7.0",
    "global": "^4.4.0",
    "handlebars": "^4.7.7",
    "helmet": "^3.21.2",
    "ioredis": "^5.2.4",
    "jest-config": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.3",
    "mongoose": "^6.5.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.8.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^19.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "7.5.5",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.7.4",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/testing": "^6.8.3",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.12.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/mongodb": "^3.3.6",
    "@types/node": "14.18.10",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "codecov": "^3.6.1",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.7",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "run-script-webpack-plugin": "^0.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^5.20.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0",
    "yarn": "^1.19.1"
  },
  "lint-staged": {
    "*.ts": [
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
